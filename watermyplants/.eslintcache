neNumber\"\r\n            type=\"text\"\r\n            onChange={onChange}\r\n            value={value.phoneNumber}\r\n            placeholder=\"Phone Number\"\r\n          />\r\n        </label>\r\n        <button  className=\"submit-btn\">\r\n          Register\r\n        </button>\r\n        <div className=\"errors\">\r\n          {/* <div>{errors.username}</div>\r\n            <div>{errors.password}</div>\r\n            <div>{errors.phoneNumber}</div> */}\r\n        </div>\r\n        <div>\r\n          Already Have An Account?\r\n          <Link to=\"/\" onClick={pageChange}>\r\n            <br></br>\r\n            Login\r\n          </Link>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}",["27","28"],"C:\\Users\\Chance\\Desktop\\SCHOOL\\Lambda-Assignments\\Build week\\frontend\\watermyplants\\src\\theme\\theme.js",["29"],"export default {\r\n     \r\n}",{"ruleId":"30","severity":1,"message":"31","line":2,"column":27,"nodeType":"32","messageId":"33","endLine":2,"endColumn":36},{"ruleId":null,"fatal":true,"severity":2,"message":"34","line":15,"column":2},{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":1,"column":1,"nodeType":"41","endLine":3,"endColumn":2},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","Parsing error: Unexpected token\n\n\u001b[0m \u001b[90m 13 | \u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 14 | \u001b[39m\u001b[36mexport\u001b[39m \u001b[36mdefault\u001b[39m \u001b[36mfunction\u001b[39m \u001b[33mRegister\u001b[39m(props)  {\u001b[0m\n\u001b[0m\u001b[31m\u001b[1m>\u001b[22m\u001b[39m\u001b[90m 15 | \u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<\u001b[39m \u001b[33mHEAD\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m    | \u001b[39m \u001b[31m\u001b[1m^\u001b[22m\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 16 | \u001b[39m  \u001b[0m\n\u001b[0m \u001b[90m 17 | \u001b[39m  \u001b[36mconst\u001b[39m { va